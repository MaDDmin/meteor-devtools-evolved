  },
  "jest": {
    "scriptPreprocessor": "./node_modules/babel-jest",
    "testFileExtensions": [
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  },
  "author": "Leonardo Venturini",
  "license": "MIT",
