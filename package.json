    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "css-loader": "^0.23.1",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.3",
    "jest-cli": "^12.0.2",
    "marsdb": "^0.6.9",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "react-hot-loader": "^4.12.19",
    "sass-loader": "^4.0.0",
